{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "buildCommand": "npm run vercel-build",
        "installCommand": "npm install --legacy-peer-deps"
      }
    }
  ],
  "routes": [{ "src": "/(.*)", "dest": "/index.html" }]
}
